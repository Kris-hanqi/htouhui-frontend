{
  "name": "hth-frontend",
  "version": "1.0.0",
  "description": "海投汇官网",
  "author": "wangxingkang <wang_xingkang@qq.com>",
  "private": true,
  "scripts": {
    "dev": "npm run bootstrap && nuxt",
    "serve-dev": "nodemon -w ./server -w ./start.js --exec node ./start.js",
    "bootstrap": "yarn || npm i",
    "build": "nuxt build",
    "start": "nuxt start",
    "server-build": "nuxt build && backpack build",
    "server-start": "cross-env NODE_ENV=production node build/main.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "ccap": "^0.6.10",
    "cross-env": "^5.1.3",
    "element-ui": "^2.0.11",
    "hth-icon-font": "^0.1.1",
    "js-cookie": "^2.2.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-connect": "^2.0.0",
    "koa-proxies": "^0.6.2",
    "koa-router": "^7.3.0",
    "koa-session": "^5.8.1",
    "mysql": "^2.15.0",
    "normalize.css": "^7.0.0",
    "nuxt": "^1.3.0",
    "source-map-support": "^0.5.2",
    "throttle-debounce": "^1.0.1",
    "trek-captcha": "^0.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-preset-latest-node": "^0.4.0",
    "babel-preset-stage-3": "^6.24.1",
    "backpack-core": "^0.5.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "postcss": "^6.0.16",
    "postcss-html": "^0.12.0",
    "sass-loader": "^6.0.6",
    "scmp": "^2.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0"
  }
